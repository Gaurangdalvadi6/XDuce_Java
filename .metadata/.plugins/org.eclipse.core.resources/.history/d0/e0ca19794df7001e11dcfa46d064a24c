package oopsConcept;

class Student {
	int roll;
	String name;

	Student(String name) {
		this.name = "Java";
	}
}

public class Constructor_Example {
	public static void main(String[] args) {
		Student s1 = new Student("Gaurang");
		Student s2 = new Student("Dev");
		System.out.println(s1.name);
		System.out.println(s2.roll);
	}
}
