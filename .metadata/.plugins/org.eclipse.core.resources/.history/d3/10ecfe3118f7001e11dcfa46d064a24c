package stringExample;

public class StringCompare {
public static void main(String[] args) {
	String str = "Gaurang";
	String str1 = "Gaurang";
	String str2 = new String("Gaurang");
	
	if (str==str1) {
		System.out.println("String are equal");
	} else {
		System.out.println("String are not equal");
	}
}
}
